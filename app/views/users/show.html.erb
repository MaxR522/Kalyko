<%- # app/views/users/show.html.erb -%>
<link rel="stylesheet" href="<%Rails.public_path%>/css_log/profil.css">
<h1>Welcome to your profile page</h1>

<div>
  <% if current_user.avatar.attached? %>
    <%= image_tag current_user.avatar, class: 'img_profile' %>
  <% end %>
</div>

<h3>Your account description:</h3>

<ul class="users">
  <li>Username: <%= current_user.username%> </li>
  <li>email: <%= current_user.email%> </li>
</ul>

<%= link_to 'Edit', edit_user_registration_path, class: 'btn btn-primary'%>

<div class="mapouter"><div class="gmap_canvas"><iframe width="600" height="500" id="gmap_canvas" src="https://maps.google.com/maps?q=Forello%20expo&t=&z=15&ie=UTF8&iwloc=&output=embed" frameborder="0" scrolling="no" marginheight="0" marginwidth="0"></iframe><a href="https://123movies-to.org">123movies</a></div>
<style>.mapouter{position:relative;text-align:right;height:500px;width:600px;}.gmap_canvas {overflow:hidden;background:none!important;height:500px;width:600px;}</style></div>